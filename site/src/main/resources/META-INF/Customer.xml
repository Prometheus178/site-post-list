<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.realestate.site.models.users.entities.Customer" table="customers" schema="real-estate-site">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="agreeToDataProcessing" column="agree_to_data_processing"/>
        <property name="comment" column="comment"/>
        <property name="dateTime" column="date_time"/>
        <property name="email" column="email"/>
        <property name="mortgage" column="mortgage"/>
        <property name="phoneNumber" column="phone_number"/>
        <many-to-one name="apartment" class="com.realestate.site.models.new_buildings.model.Apartment" lazy="false">
            <column name="new_building_id"/>
        </many-to-one>
    </class>
</hibernate-mapping>